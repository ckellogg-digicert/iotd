<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.github.thoom.iotd</string>

    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/iotd</string>
    </array>

    <key>Nice</key>
    <integer>1</integer>

    <key>StartCalendarInterval</key>
    <dict>
      <key>Minute</key>
      <integer>00</integer>
    </dict>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardErrorPath</key>
    <string>/tmp/iotd.out</string>

    <key>StandardOutPath</key>
    <string>/tmp/iotd.out</string>
  </dict>
</plist>